{
  "name": "react-ecosystem-aurora",
  "version": "0.0.1",
  "description": "react/webpack/hot-loader/babel + more project ecosystem",
  "scripts": {
    "build": "NODE_ENV=production webpack -p --progress --colors --config webpack.production.config.js",
    "build-dev": "BUILD_DEV=1 webpack --config webpack.development.config.js -d --progress --colors",
    "server": "NODE_ENV=production nodemon ./server/server.js",
    "server-dev": "nodemon --watch server ./server/server.js",
    "production": "sudo npm run server",
    "watch": "BUILD_DEV=1 webpack-dev-server --config webpack.development.config.js --devtool eval --progress --colors --hot --inline --port 3001 --content-base public/build/development",
    "start": "npm run server-dev & npm run watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/firegoby/react-ecosystem-aurora"
  },
  "author": "Chris Batchelor (@cwbat)",
  "license": "MIT",
  "dependencies": {
    "autoprefixer-loader": "^1.2.0",
    "babel-core": "^5.0.4",
    "babel-loader": "^5.0.0",
    "css-loader": "^0.9.1",
    "debug": "^2.1.3",
    "ejs": "^2.3.1",
    "express": "^4.12.3",
    "immutable": "^3.7.1",
    "jsx-loader": "^0.12.2",
    "nodemon": "^1.3.7",
    "react": "^0.13.1",
    "react-router": "^0.13.2",
    "request": "^2.53.0",
    "style-loader": "^0.8.3",
    "stylus-loader": "^1.0.0",
    "webpack": "^1.7.3"
  },
  "devDependencies": {
    "react-hot-loader": "^1.2.3",
    "webpack-dev-server": "^1.7.0"
  }
}
